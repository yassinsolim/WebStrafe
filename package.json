    "@types/three": "^0.182.0",
    "commander": "^14.0.3",
    "concurrently": "^9.2.1",
    "gltf-pipeline": "^4.3.0",
    "obj2gltf": "^3.2.0",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
